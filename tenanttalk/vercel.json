{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/build/**",
        "use": "@vercel/static",
        "config": {
          "distDir": "Frontend/build"
        }
      },
      {
        "src": "Backend/server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Backend/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend/build/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }